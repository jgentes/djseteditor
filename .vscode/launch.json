{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug",
      "type": "pwa-msedge",
      "request": "launch",
      "preLaunchTask": "Parcel",
      "url": "http://localhost:1234",
      "webRoot": "${workspaceFolder}",
      "sourceMaps": true,
      "skipFiles": [
        "/node_modules/**/*.{ts,js}",
        "**/<node_internals>/**/*.{ts,js}"
      ],
      "outFiles": ["/dist/*.js", "!**/node_modules/**"]
    }
  ]
}
